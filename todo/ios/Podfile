# See: https://guides.cocoapods.org/syntax/podfile.html#platform
platform :ios, '11.0'

target 'ToDoSync' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!

  # Pod for MongoDB / Stitch
    pod 'StitchSDK', '~> 5.0.0'

  # UI Extras
    pod 'Toast-Swift', '~> 4.0.0'
    pod 'BEMCheckBox'

end
